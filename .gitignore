cat > .gitignore << 'EOF'
# Python
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
venv/
env/
ENV/
.venv

# Jupyter Notebook
.ipynb_checkpoints

# Data files
# *.csv
*.xlsx
*.json
data/raw/*
!data/raw/.gitkeep

# Model files - KEEP our trained model!
# models/*.pkl
# models/*.joblib

# IDE
.vscode/
.idea/
*.swp
*.swo

# OS
.DS_Store
Thumbs.db

# Outputs - KEEP our visualizations!
# outputs/*.png
# outputs/*.pdf
!outputs/.gitkeep
# Track these generated artifacts on GitHub
EOF